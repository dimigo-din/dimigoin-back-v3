{
  "name": "dimigoin-back-v3",
  "version": "1.0.0",
  "description": "DIMIGOin Backend Application",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "cross-env NODE_ENV=develop tsc-watch --onSuccess 'node dist/index.js'",

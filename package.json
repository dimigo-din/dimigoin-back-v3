    "lint:fix": "eslint --fix src/**/*.ts",
    "serve": "cross-env NODE_ENV=prod node dist/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix"
    ]
  },
  "author": "Seohun Uhm <uhmtotow@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "nodemon": "^1.18.9",
    "tsc-watch": "^4.2.3"
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/express-fileupload": "^1.1.2",
    "@types/helmet": "^0.0.45",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/mongoose": "^5.7.6",
    "aws-sdk": "^2.639.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dot-prop": "^4.2.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-bearer-token": "^2.4.0",
    "express-fileupload": "^1.1.6",
    "helmet": "^3.21.3",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.0",
    "mongoose": "^5.9.4",
    "ts-mongoose": "^0.0.21",
    "typescript": "^3.9.7",
    "bl": "^2.2.1"
  }
